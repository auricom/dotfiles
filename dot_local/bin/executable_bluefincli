#!/bin/sh
# distrobox_binary
# name: bluefin-cli
if [ -z "${CONTAINER_ID}" ]; then
	exec "distrobox-enter"  -n bluefin-cli  --  '/usr/bin/fish'  "$@"
elif [ -n "${CONTAINER_ID}" ] && [ "${CONTAINER_ID}" != "bluefin-cli" ]; then
	exec distrobox-host-exec '/var/home/devbox/.local/bin/fish'  "$@"
else
	exec '/usr/bin/fish' "$@"
fi
