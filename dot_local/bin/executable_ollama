#!/bin/sh
# distrobox_binary
# name: bluefin-cli
if [ -z "${CONTAINER_ID}" ]; then
	exec "distrobox-enter"  -n bluefin-cli  --  '/home/linuxbrew/.linuxbrew/bin/ollama'  "$@"
elif [ -n "${CONTAINER_ID}" ] && [ "${CONTAINER_ID}" != "bluefin-cli" ]; then
	exec distrobox-host-exec '/var/home/claude/.local/bin/ollama'  "$@"
else
	exec '/home/linuxbrew/.linuxbrew/bin/ollama' "$@"
fi
